		</div>
		<div class="bot_box med_pad_bot pad_top">
			<p>&nbsp &nbsp &nbsp &nbsp &nbsp </p>
		</div>				
	</body>
	<script>
		$(document).ready(function(){
		function change_resp_credits(){
			$(".close_resp_this").fadeToggle(200); 
		} 
		function change_lead_credits(){
			$(".close_lead_this").fadeToggle(200);
		}
		// change_credits_init() 
		$('.close_resp').on("click",change_resp_credits)
		$('.close_lead').on("click",change_lead_credits)
	});
	</script>
